from random import randint
a = []
for i in range(10):
  a.append(randint(1, 50))
def qsort(a):
    if len(a)<=1:
        return a
    elem = a[0]
    left = list(filter(lambda x: x< elem, a))
    midle = [i for i in a if i== elem]
    right = list(filter(lambda x: x > elem, a))
    return qsort(left) + midle + qsort(right)
a=qsort(a)
print(a)
