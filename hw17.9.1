def bsearch(a, value):
    mid = len(a)//2
    low = 0
    high = len(a)
    if a[0] >= value or value>= a[-1]:
        raise Exception('Значение не соответствует условиям')
    while low < high-1:
        print(low,mid,high, value, a[mid])
        if value >= a[mid]:
          low = mid 
        else:
          high = mid 
        mid = (low + high)//2
    if value < a[high-1]:
         mid = high - 1
    return mid 
